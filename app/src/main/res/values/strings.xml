    <string name="onboarding_search_content_survey">We\'re working hard to offer this feature as soon as possible. You can be the first one trying and help us improve the feature.</string>
    <string name="onboarding_search_action_agree">OK, I\'m in</string>
    <string name="onboarding_search_action_disagree">No, thanks</string>
    <string name="onboarding_search_content_feedback"></string>

