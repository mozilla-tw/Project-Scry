    <string name="about_list_right">Your Rights/ Licenses</string>
    <string name="about_list_privacy">Privacy Notice</string>

    <!-- Onboarding: strings to onboard users to grant permissions,
    Refer to UX spec 7.1: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133929_7-1_Onboarding Parameter %1$s is the app name, i.e. Screenshot Go -->
    <string name="onboarding_storage_title_welcome">Welcome to %1$s</string>
    <string name="onboarding_storage_content_permission">We need your permission to load your current screenshots from Gallery to do the magics.</string>
    <string name="onboarding_storage_action_confirm">Let\'s load it!</string>
    <string name="onboarding_transition_loading">Loading</string>
