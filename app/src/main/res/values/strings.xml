    <string name="search_transition_searching">Searching&#8230;</string>
    <string name="search_error_title_noresult">No results found</string>
    <string name="search_error_content_try">Try another word</string>
    <!-- Parameter %1$d is the quantity of matched search results, i.e. 100 results -->
    <string name="search_separator_results">%1$d results</string>
    <!-- String displayed to inform users the progress of text recognition. Parameter %1$d is the quantity of screenshot files, i.e. 100 screenshots remaining -->
    <string name="search_transition_progress">%1$d screenshots remaining&#8230;</string>
