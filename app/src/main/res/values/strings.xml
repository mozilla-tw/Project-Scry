<resources>
    <!-- Label used for buttons to create new collection, e.g. in dialogs-->
    <string name="action_create">Create new collection</string>
    <!-- Label used for buttons to delete items, e.g. in dialogs-->
    <string name="action_delete">Delete</string>
    <!-- Label used for buttons to share selected items, e.g. selected text, selected screenshot, or share the app-->
    <string name="action_share">Share</string>
    <!-- Label used for title in screenshot info dialogue, but for action in menu -->
    <string name="info_info">Screenshot info</string>
    <!-- Label used when no content -->
    <string name="title_noshots">No screenshots yet!</string>
    <!-- Label used for buttons to skip the current action -->
    <string name="action_later">Later</string>

    <!-- Notification: strings displayed on the notification to perform various actions -->
    <string name="notification_action_capture">Tap to take screenshot</string>
    <string name="notification_action_open">Open app</string>
    <string name="notification_action_stop">Stop</string>
    <!-- String displayed to inform users new screenshots captured -->
    <string name="notification_action_collect_title">New screenshots captured</string>
    <string name="notification_action_collect">Tap to sort your screenshots</string>

    <!-- Sorting panel: strings displayed on the sorting panel to help users categorize screenshots, 
    Refer to UX spec 1.1: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133909_1-1_Key_Flow_Add_To_Collection -->
    <!-- String displayed to skip the current screenshot and move on to the next one -->
    <string name="multisorting_action_next">Next</string>
    <!-- String displayed on the title of sorting panel and the screenshot will be categorized to a collection -->
    <string name="sorting_title_move">Move to</string>
    <!-- Strings displayed to suggest default collections for users to sort -->
    <string name="sorting_suggestion_1st">Shopping</string>
    <string name="sorting_suggestion_2nd">Finance &amp; Banking</string>
    <string name="sorting_suggestion_3rd">News &amp; Article</string>
    <string name="sorting_suggestion_4th">Chat history</string>
    <string name="sorting_suggestion_5th">Event &amp; Activity</string>
    <!-- Strings displayed to allow users to create collection name -->
    <string name="dialogue_title_collection">Collection name</string>
    <!-- String displayed as a hint to ask users to type a name -->
    <string name="dialogue_inputfield_hint">Give me a name</string>
    <!-- String displayed to create a new collection and add the screenshot into it -->
    <string name="dialogue_action_add">Add</string>

    <!-- Home view: strings displayed on the home view,
    Refer to UX spec 2.1: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133914_2-1_Navigation-->
    <string name="home_action_search">Search</string>
    <string name="menu_home_action_settings">Settings</string>
    <string name="home_separator_access">Quick access</string>
    <string name="home_action_more">View all</string>
    <string name="home_separator_collection">Collections</string>
    <!-- String displayed to allow users to enter a collection with all unsorted screenshots -->
    <string name="home_action_unsorted">Unsorted</string>

    <!-- Collection view: string displayed on the collection view. Parameter %1$d is the quantity of screenshot files, i.e. 100 shots -->
    <string name="collection_separator_shots">%1$d shots</string>
    <!-- String displayed to inform users all screenshots are selected -->
    <string name="collection_header_select_all">All selected</string>
    <!-- String displayed to inform users to select screenshot -->
    <string name="collection_header_select_none">Select screenshots</string>
    <string name="collection_header_viewall_all">All screenshots</string>
    <!-- String displayed when text recognition has an unknown error -->
    <string name="detail_ocr_error_failed">Text recognition failed</string>
    <!-- String displayed when text recognition fails because the image is too big or has unacceptable proportion -->
    <string name="detail_ocr_error_edgecase">Something went wrong? Crop the screenshot and retry.</string>
    <!-- String displayed when recognizing no text on the screenshot -->
    <string name="detail_ocr_error_notext">No text found</string>
    <!-- String displayed to ask users to connect to internet to install necessary module -->
    <string name="detail_ocr_error_module">Connect to the internet to install offline Text Recognition module</string>
    <!-- String displayed to direct users to Network & internet in system Settings -->
    <string name="detail_ocr_error_action_connect">Connect</string>
    <!-- String displayed to give users the instruction -->
    <string name="detail_ocr_instruction">Tap text for more options</string>
    <string name="detail_ocr_action_selectall">Select all text</string>
    <!-- Detail view: string displayed on the detail view when recognizing the text on the screenshot -->
    <string name="detail_transition_recognizing">Recognizing text&#8230;</string>

    <!-- Dialogue: strings displayed in the dialogues which inform users about a task, e.g. dialogue title, content and action -->
    <!-- This string also serves as menu action in collection view -->
    <string name="dialogue_collecitioninfo_title_info">Collection info</string>
    <!-- String displayed as a tag to accomodate the name attribute, e.g. Name: shopping -->
    <string name="dialogue_list_name">Name</string>
    <string name="dialogue_collecitioninfo_list_shot">Total screenshots</string>
    <string name="dialogue_collecitioninfo_list_storage">Storage used</string>
    <string name="dialogue_shotinfo_list_size">Size</string>
    <string name="dialogue_list_edit">Last edit</string>
    <string name="dialogue_deletecollection_title_delete">Delete collection?</string>
    <string name="dialogue_deleteshot_title_delete">Delete screenshot?</string>
    <!-- String displayed to state that the action will erase "all screenshots in the collection," which is "its screenshots" in the string. -->
    <string name="dialogue_delete_content_cantundo">Are you sure? Deleting this collection will permanently remove all of its screenshots from your phone.</string>
    <!-- String displayed to state that the action will erase all selected screenshots. -->
    <string name="dialogue_deleteshot_content_delete">Are you sure? Deleting screenshots will permanently remove screenshots from your phone.</string>
    <!-- Parameter %1$d is the quantity of screenshot files, i.e. 100 screenshots -->
    <string name="dialogue_deletecollection_content_shots">%1$d screenshots</string>
    <string name="dialogue_rename_title_rename">Rename collection</string>
    <string name="dialogue_rename_error_duplicate">There is already a collection with this name</string>
    <string name="dialogue_rename_error_maximum">20 characters maximum</string>
    <!-- Dialogue: Strings displayed when users skip multi-sorting panel,
    Refer to UX spec 2.4_E: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133917_2-4_Detect_New_Screenshots-->
    <string name="dialogue_skipsorting_title_skip">Skip the rest?</string>
    <string name="dialogue_skipsorting_content_moveto">Skip remaining screenshots and move to “Unsorted?”</string>
    <!-- Parameter %1$d is the quantity of screenshot files, i.e. 100 remaining -->
    <string name="dialogue_skipsorting_content_count">%1$d remaining</string>
    <!-- String to allow users to skip sorting the remaining screenshots -->
    <string name="dialogue_skipsorting_action_skip">Skip all</string>

    <!-- Menu: strings displayed when triggering the overflow menu in the top-right corner of each view -->
    <!-- String to allow users to multi-select screenshots -->
    <string name="menu_action_select">Select</string>
    <string name="menu_shot_action_move">Move to&#8230;</string>
    <string name="menu_action_rename">Rename</string>
    <!-- Strings displayed to perform actions when selecting text on the screenshot,
    Refer to UX spec 4.2_G&I: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133922_4-2_Image_-_Text_Mode-->
    <string name="menu_ocr_action_web">Web search</string>
    <string name="menu_ocr_action_copy">Copy</string>
    <string name="menu_action_selectall">Select all</string>

    <!-- Bottom sheet: strings displayed on the reminder panel, and the placeholder %1$s is the app name, ScreenshotGo -->
    <string name="sheet_enable_title_enable">Enable %1$s to take screenshots?</string>
    <string name="sheet_enable_content_enable">You won’t be able to take screenshots with one tap unless this is enabled.</string>
    <!-- String displayed to allow users to enable the screenshot service -->
    <string name="sheet_enable_action_enable">Enable</string>
    <string name="sheet_unsorted_title_unsorted">Unsorted screenshots</string>
    <!-- Parameter %1$d is the quantity of screenshot files, i.e. 100 unsorted screenshots. Sort them now? -->
    <string name="sheet_unsorted_content_shots">%1$d unsorted screenshots. Sort them now?</string>
    <string name="sheet_box_ask">Don’t ask again</string>
    <string name="sheet_unsorted_action_sort">Sort</string>
    <string name="sheet_action_no">No thanks</string>

    <!-- Snackbar: strings to provide confirmations for user’s actions -->
    <string name="snackbar_disable">Screenshot feature disabled</string>
    <string name="snackbar_enable">Screenshot feature enabled</string>
    <!-- Parameter %1$s is the collection name user defined, i.e. Shopping -->
    <string name="snackbar_addto">Added to %1$s</string>
    <string name="snackbar_copied">Copied</string>

    <!-- Settings: strings displayed in the settings page,
    Refer to UX spec 6.1: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133927_6-1_Settings -->
    <!-- the string displayed to inform users this switch needs to be enabled to take screenshots. If enabled, the system reveals two more options for users to customize -->
    <string name="settings_list_capture">Enable screenshot feature</string>
    <!-- The string changes to serve as description of settings_list_fab -->
    <string name="settings_detail_capture">Allows you to take a screenshot with one tap. Works with whatever is on your screen.</string>
    <!-- The string displayed to enable a floating button to take screenshot, and the placeholder %1$s is "GO" from the product name -->
    <string name="settings_list_fab">Use %1$s button</string>
    <string name="settings_list_sorting">Prompt to sort</string>
    <string name="settings_detail_sorting">Always prompt to add screenshot to a collection</string>
    <string name="settings_list_usagedata">Send usage data</string>
    <!-- Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="settings_detail_mozilla">Mozilla strives to collect only what we need to provide and improve %1$s for everyone.</string>
    <string name="settings_action_learnmore">Learn more</string>
    <string name="settings_list_feedback">Give feedback</string>
    <string name="feedback_title_lovefeedback">We love feedback!</string>
    <string name="feedback_detail_letusknow">Let us know what you think.</string>
    <string name="feedback_action_rate">Yes, rate 5 stars!</string>
    <string name="feedback_action_feedback">No, send feedback.</string>
    <string name="settings_list_share">Share with friends</string>
    <!-- The strings displys when users share the app with friends. %1$s is the app name i.e. Firefox ScreenshotGo, and %2$s is the Google Play url -->
    <string name="share_intro">Hey, check out this new app that helps you organize and find your screenshots easily. It’s called %1$s. %2$s</string>
    <string name="settings_list_about">About</string>
    <string name="about_title_name" translatable="false">@string/app_full_name</string>
    <!-- Parameter %1$s is version code of the app, i.e. 1.0.0 -->
    <string name="about_content_version">Version %1$s</string>
    <string name="about_list_support">Support</string>
    <string name="about_list_right">Your Rights</string>
    <string name="about_list_privacy">Privacy Notice</string>

    <!-- Onboarding: strings to onboard users to grant permissions,
    Refer to UX spec 7.1: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133929_7-1_Onboarding Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="onboarding_storage_title_welcome">Welcome to %1$s</string>
    <string name="onboarding_storage_content_permission">To help you organize and find your screenshots better, we’ll need your permission to access phone storage.</string>
    <string name="onboarding_storage_caption_duplicates">*Don’t worry, this won’t duplicate files.</string>
    <!-- The string displays if the system autogrants storage permission. Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="onboarding_storage_content_autogrant">%1$s helps you organize and find your screenshots better.</string>
    <string name="onboarding_storage_action_confirm" translatable="false">Let’s GO</string>
    <string name="onboarding_transition_loading">Loading</string>
    <!-- Overlay permission prompt: strings to onboard users to grant permissions, and the placeholder %1$s is "GO" from the product name -->
    <string name="onboarding_fab_title_fab">Get the %1$s button</string>
    <!-- The placeholder %1$s is "GO" from the product name -->
    <string name="onboarding_fab_content_permission">The %1$s button lets you take and organize your screenshots anytime, anywhere. We’ll need your permission to enable it.</string>
    <!-- Auto-grant overlay permission prompt. The placeholder %1$s is "GO" from the product name -->
    <string name="onboarding_autogrant_overlay_title">Use the %1$s button to take and organize screenshots anytime, anywhere.</string>
    <string name="onboarding_autogrant_overlay_gotit">Got it</string>
    <!-- Onboarding: strings to inform users when keeping denying permission -->
    <string name="onboarding_error_title_wrong">Oops! Something’s wrong</string>
    <!-- Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="onboarding_error_content_permission">%1$s needs this permission to work correctly.</string>
    <string name="onboarding_error_action_allow">Allow permission</string>
    <!-- Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="onboarding_rareerror_content_permission">%1$s needs permission to access your phone’s storage. Please find Permission to turn on Storage.</string>
    <string name="onboarding_rareerror_action_goto">Go to permission</string>
    <!-- Onboarding: strings to inform users that the search feature is WIP,
    Refer to UX spec 7.3: https://mozilla.invisionapp.com/share/24N3MMQB986#/310133930_7-3_Other_Oborading -->
    <string name="onboarding_search_title_soon">Search is coming soon!</string>
    <!-- The string displayed to describe the survey -->
    <string name="onboarding_search_content_survey">We’re working on a magic feature to help you search screenshots with text. Interested?</string>
    <string name="onboarding_search_action_agree">Yes please</string>
    <!-- The string should represent an informal variant of no -->
    <string name="onboarding_search_action_disagree">Nope</string>
    <string name="onboarding_search_content_feedback">Thanks for your feedback!</string>
    <!-- String displayed with an illustration to introduce a new feature -->
    <string name="onboarding_ocr_title_more">Take Action on Text</string>
    <!-- String displayed to give examples of what the new feature can do -->
    <string name="onboarding_ocr_content_examples">Copy the text in your screenshot for web search, share or more.</string>
    <!-- Onboarding: strings displayed with an illustration to introduce users the new search feature, Refer to UX spec 5-1 https://mozilla.invisionapp.com/share/24N3MMQB986#/310133925_5-1_Search -->
    <string name="onboarding_search_title_search">Finding screenshots is now easier</string>
    <!-- String displayed to instruct users to type words to search -->
    <string name="onboarding_search_content_typeword">Just type to search words within screenshots</string>
    <!-- String displayed to inform users recognition costs more battery -->
    <string name="onboarding_search_caption_battery">To make this happen, we’ll recognize your screenshot first. It may consume more battery, but it won’t take long!</string>
    
    <!-- Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="dialogue_feedback_title">Love %1$s?</string>
    <!-- Parameter %1$s is the app name, i.e. Firefox ScreenshotGo -->
    <string name="dialogue_share_title">Share %1$s with Friends!</string>
    <string name="dialogue_take_share_description">Enjoy taking screenshots? Share it now!</string>
    <string name="dialogue_sort_share_description">Enjoy organizing screenshots? Share it now!</string>
    <string name="dialogue_ocr_share_description">Find screenshot-to-text useful? Share it now!</string>
