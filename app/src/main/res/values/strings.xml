    <string name="onboarding_fab_title_fab">Easiest Way to Screenshot</string>
    <string name="onboarding_fab_content_permission">We need some permissions to let you take screenshots with a floating screenshot button.</string>
    <string name="onboarding_fab_action_later">Later</string>
    <string name="onboarding_fab_action_grant">Go to grant page</string>
